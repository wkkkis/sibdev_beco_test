                        <div class="userid-option options-sort-block option-sort-disabled">
                            <div class="arrow-for-sort">
                            </div>
                            <div class="option-sort-input">
                                {{ field("User ID", "text", "From", "from_id_input") }}
                                {{ field("", "text", "To", "to-id-input") }}
                            </div>
                            <div class="option-sort-button">
                                <div class="reset-btn">
                                    {{ button("Reset filter", "light", "button", "reset_userid", "text-15") }}
                                </div>
                                {{ button("Apply filter", "dark", "button", "apply_userid", "text-15") }}
                            </div>
                        </div>
                    </div>
                    <div class="sort-block">
                        {{ options_sort("Name / Surname", "name") }}
                        <div class="name-option options-sort-block option-sort-disabled">
                            <div class="arrow-for-sort">
                            </div>
                            {{ field("Name", "text", "Enter name", "275px", "from_name_input") }}
                            {{ field("Surname", "text", "Enter surname", "275px", "to_name_input") }}
                            <div class="option-sort-button">
                                <div class="reset-btn">
                                    {{ button("Reset filter", "light", "button", "reset_name", "text-15") }}
                                </div>
                                {{ button("Apply filter", "dark", "button", "apply-name", "text-15") }}
                            </div>
                        </div>
                    </div>
                    <div class="sort-block">
                        {{ options_sort("Role", "role") }}
                        <div class="role-option options-sort-block option-sort-disabled">
                            <div class="arrow-for-sort">
                            </div>
                            <div>
                                <label class="label-for-drop">Role</label>
                                <select id="role-select" class="select">
                                    <option disabled>Select role (-es)</option>
                                    <option value="1">Super Admin</option>
                                    <option value="2">Admin</option>
                                    <option value="3">Announcement worker</option>
                                    <option value="4">Feedback & Support worker</option>
                                    <option value="5">Worker 1</option>
                                    <option value="5">Worker 2</option>
                                </select>
                            </div>
                            <div class="option-sort-button">
                                <div class="reset-btn">
                                    {{ button("Reset filter", "light", "button", "reset_email", "text-15") }}
                                </div>
                                {{ button("Apply filter", "dark", "button", "apply_email", "text-15") }}
                            </div>
                        </div>
                    </div>
                    <div class="sort-block">
                        {{ options_sort("Created", "created") }}
                        <div class="created-option options-sort-block right option-sort-disabled">
                            <div class="arrow-for-sort">
                            </div>
                            <div class="option-sort-input">
                                <div style="margin-right: 20px">
                                    <label class="label-for-drop">Created</label>
                                    <input placeholder="From"
                                           type="text"
                                           onfocus="(this.type = 'date')"
                                           id="from_created_input">
                                </div>
                                <input placeholder="To"
                                       type="text"
                                       onfocus="(this.type = 'date')"
                                       id="to_created_input">
                            </div>
                            <div class="option-sort-button">
                                <div class="reset-btn">
                                    {{ button("Reset filter", "light", "button", "reset_created", "text-15") }}
                                </div>
                                {{ button("Apply filter", "dark", "button", "apply-created", "text-15") }}
                            </div>
                        </div>
                    </div>
                    <div class="sort-block">
                        {{ options_sort("Status", "status") }}
                        <div class="status-option options-sort-block right option-sort-disabled">
                            <div class="arrow-for-sort">
                            </div>
                            <div>
                                <label class="label-for-drop">Select status</label>
                                <select id="role_status-select" class="select">
                                    <option disabled>Select status</option>
                                    <option value="1">Select status 1</option>
                                    <option value="2">Select status 2</option>
                                    <option value="3">Select status 3</option>
                                    <option value="4">Select status 4</option>
                                    <option value="5">Select status 5</option>
                                    <option value="5">Select status 6</option>
                                    <option value="5">Select status 7</option>
                                </select>
                            </div>
                            <div class="option-sort-button">
                                <div class="reset-btn">
                                    {{ button("Reset filter", "light", "button", "reset_status", "text-15") }}
                                </div>
                                {{ button("Apply filter", "dark", "button", "apply_status", "text-15") }}
                            </div>
                        </div>
                    </div>
                </div>
                <div class="data-lines tbody">
                    {% for i in range(1, 10) %}
                        <div class="rules-line th-line">
                            <div class="checkbox-custom th-checkbox">
                                <input type="checkbox"
                                       id="checkbox{{ i }}"
                                       class="selectedId">
                                <label for="checkbox{{ i }}"></label>
                            </div>
                            <div class="th-line-padding bold">
                                <span>00000{{ i }}</span>
                            </div>
                            <div class="th-line-padding">
                                <span>Johnathan Silverstone</span>
                            </div>
                            <div class="th-line-padding">
                                <span>Super Admin</span>
                            </div>
                            <div class="th-line-padding">
                                <span>20 may 2020</span>
                            </div>
                            <div class="th-line-padding">
                                <span>Active</span>
                            </div>
                        </div>
                    {% endfor %}
                </div>
            </div>
        </div>
    </div>
{% endblock %}
