                        <div class="button text-13 green">
                            99+
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="feedback-content">
            <div class="users-chat">
                <div class="sort-block">
                    {{ options_sort("Filter", "150px", "filter") }}
                    <div class="filter-option options-sort-block option-sort-disabled">
                        <div class="arrow-for-sort">
                        </div>
                        {{ field("Name", "text", "Enter name", "290px", "filter_name") }}
                        {{ field("Surname", "text", "Enter surname", "290px", "filter_surname") }}
                        <label class="label-for-drop">Category</label>
                        <select id="filter_category" class="select">
                            <option disabled>Select city / cities</option>
                            <option value="1">Select status 1</option>
                            <option value="2">Select status 2</option>
                            <option value="3">Select status 3</option>
                            <option value="4">Select status 4</option>
                            <option value="5">Select status 5</option>
                            <option value="5">Select status 6</option>
                            <option value="5">Select status 7</option>
                        </select>
                        <div class="option-sort-button">
                            <div class="reset-btn">
                                {{ button("Reset filter", "light", "button", "reset_name", "text-15") }}
                            </div>
                            {{ button("Apply filter", "dark", "button", "apply-name", "text-15") }}
                        </div>
                    </div>
                </div>
                <div class="chats-link active">
                    <div class="chats-link__title">
                        <span>Set of two Modern Chairs with foot stand</span>
                        <span>Johnathan Silverstone (ID 000001)</span>
                    </div>
                    <span class="chats-link__notification">
                        2
                    </span>
                    <div class="logo-with-message">
                        <div class="logo-with-message__logo">
                            <img src="../static/images/rantage.svg" alt="">
                            <div class="user-active">
                                <div class="user-active__circle">
                                </div>
                            </div>
                        </div>
