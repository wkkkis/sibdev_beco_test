                            </svg>
                            <span>Add category</span>
                        </button>
                        {{ edit("88px", "categories-edit") }}
                        {{ active("105px", "categories-active") }}
                        {{ deactive("119px", "categories-deactive") }}
                        {{ export("143px") }}
                        {{ remove("97px", "categories-remove") }}
                    </div>
                    <div class="categories-modal-block">
                        {% include "blocks/export_excel.njk" %}

