{% extends "templates/main.njk" %}

{% set title = "categories" %}

{% from "macros/pages_options.njk" import pages_title, page_description %}
{% from "macros/tools.njk" import edit, block, unblock, export, remove, active, deactive %}
{% from "macros/users_options.njk" import options_sort %}
{% from "macros/users_options.njk" import checkbox %}
{% from "macros/input_custom.njk" import field %}
{% from "macros/button_custom.njk" import button %}

{% block content %}
    <div class="categories-wrap-main">
        {{ pages_title("Categories", "Showing 1 to 10 of 10 categories") }}
        <div class="categories-content">
            <div class="tool-wrap text-13">
                <div class="tool-bar">
                    <div class="tool-bar__title">
                        <span>Tools :</span>
                    </div>
                    <div class="tool-bar__links categories-tools">
                        <button disabled class="tool-btn categories-add button disabled" style="width: 133px;">
